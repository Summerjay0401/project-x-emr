{
  "name": "project-x-emr",
  "version": "1.0.0",
  "engines": {
    "node": "14.16.0"
  },
  "description": "![Application Logo](./src/project-emr-frontend/src/assets/images/my-pulse-logo.png)",
  "main": "index.js",
  "scripts": {
    "start": "cd src/project-emr-backend && npm run start",
    "dev:server": "cd src/project-emr-backend && npm run start",
    "dev:client": "cd src/project-emr-frontend && npm run start",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "babel --delete-dir-on-start --out-dir dist --copy-files --ignore \"**/__tests__/**,**/__mocks__/**\" --no-copy-ignored src/project-emr-backend",
    "generate": "cd src/project-emr-backend && npm run seed",
    "postinstall": "cd src/project-emr-frontend && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Summerjay0401/project-x-emr.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Summerjay0401/project-x-emr/issues"
  },
  "homepage": "https://github.com/Summerjay0401/project-x-emr#readme",
  "dependencies": {
    "nodemon": "^2.0.20"
  },
  "devDependencies": {
    "concurrently": "^7.5.0"
  }
}
